    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Launch with no arg",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/peeler.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch with loop",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/peeler.js -l 1000"
        }
    ]
}